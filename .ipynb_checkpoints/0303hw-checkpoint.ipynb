   "source": [
    "# 輸入日期查詢平閏年\n",
    "## 利用if和elif條件判斷"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2020\n",
      "閏年\n"
     ]
    }
   ],
   "source": [
    "inputyear = int(input())\n",
    "if inputyear %4 != 0:\n",
    "    print(\"平年\")\n",
    "elif inputyear %100 != 0:\n",
    "    print(\"閏年\")\n",
    "elif inputyear %400 == 0 :\n",
    "    print(\"閏年\")\n",
    "else:\n",
    "    print(\"平年\")"
   ]
  },
